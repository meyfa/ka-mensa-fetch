{
  "name": "ka-mensa-fetch",
  "version": "2.1.5",
  "description": "Karlsruhe (KIT) Mensa meal plan fetcher",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "data/**/*"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc && copyfiles -u 1 \"data/**/*\" dist/data && copyfiles LICENSE README.md dist && node setup-package.js",
    "build": "npm run clean && npm run compile",
    "lint": "eslint --ignore-path .gitignore . && tsc --noEmit",
    "lint-fix": "eslint --fix --ignore-path .gitignore . && tsc --noEmit",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.test.*\"",
    "coverage": "nyc --reporter=lcov --all --include \"src/**/*.ts\" npm test",
    "prepack": "echo \"Must pack/publish from ./dist\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/ka-mensa-fetch.git"
  },
  "keywords": [
    "karlsruhe",
    "mensa",
    "canteen",
    "api",
    "fetcher"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/ka-mensa-fetch/issues"
  },
  "homepage": "https://github.com/meyfa/ka-mensa-fetch#readme",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.22",
    "@types/chai-as-promised": "7.1.4",
    "@types/mocha": "9.0.0",
    "@types/node": "16.11.21",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "axios-mock-adapter": "1.20.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "copyfiles": "2.4.1",
    "eslint": "7.32.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "36.1.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.1",
    "mocha": "9.1.3",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "cheerio": "^1.0.0-rc.10",
    "moment": "^2.29.1"
  }
}
