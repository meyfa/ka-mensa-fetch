{
  "name": "ka-mensa-fetch",
  "version": "3.1.0",
  "description": "Karlsruhe (KIT) Mensa meal plan fetcher",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "tsc --noEmit -p tsconfig.lint.json && eslint --ignore-path .gitignore .",
    "lint-fix": "tsc --noEmit -p tsconfig.lint.json && eslint --fix --ignore-path .gitignore .",
    "test": "mocha --require ts-node/register --recursive \"test/**/*.test.*\"",
    "coverage": "c8 --reporter=lcov --reporter=text --all --include src --exclude \"src/types/\" npm test",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/ka-mensa-fetch.git"
  },
  "keywords": [
    "karlsruhe",
    "mensa",
    "canteen",
    "api",
    "fetcher"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/ka-mensa-fetch/issues"
  },
  "homepage": "https://github.com/meyfa/ka-mensa-fetch#readme",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "devDependencies": {
    "@meyfa/eslint-config": "2.0.2",
    "@types/chai": "4.3.3",
    "@types/chai-as-promised": "7.1.5",
    "@types/mocha": "9.1.1",
    "@types/node": "16.11.56",
    "axios-mock-adapter": "1.21.2",
    "c8": "7.12.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "eslint": "8.23.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "mocha": "10.0.0",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "axios": "^0.27.0",
    "cheerio": "^1.0.0-rc.10",
    "moment": "^2.29.1"
  }
}
